$sms = app('easysms');
try {
    $sms->send(15365504091, [
         'template' => 'SMS_185555933',
         'data' => [
             'code' => 1234
         ],
    ]);
} catch (\Overtrue\EasySms\Exceptions\NoGatewayAvailableException $exception) {
    $message = $exception->getException('aliyun')->getMessage();
    dd($message);
}